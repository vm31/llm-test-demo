{
  "compilerOptions": {
    "target": "ES2022", // Supports modern Node.js features
    "module": "CommonJS",
    "lib": ["ES2022"],
    "types": ["node"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true
  },
    "include": [
      "cypress/**/*.ts"
    ]
  }
  